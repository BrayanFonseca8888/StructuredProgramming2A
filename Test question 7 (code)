/*Brayan_Fonseca_test_unit1*/
#include <stdio.h>
int main()
{
    int A, B, C;
  
    printf("Enter three numbers: ");
    scanf("%d %d %d", &A, &B, &C);
  
    if (A >= B) {
        if (A >= C)
            printf("%d is the greatest number.", A);
        else
            printf("%d is the greatest number.", C);
    }
    else {
        if (B >= C)
            printf("%d is the greatest number.", B);
        else
            printf("%d is the greatest number.", C);
    }
  
    return 0;
}
